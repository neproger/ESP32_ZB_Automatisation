CONFIG_HTTPD_WS_SUPPORT=y
# Keep WS streaming off by default on S3 backend; enable from menuconfig when needed
# CONFIG_GW_HTTP_WS_ENABLE is not set

CONFIG_SPIRAM_MALLOC_ALWAYSINTERNAL=8192
CONFIG_SPIRAM_TRY_ALLOCATE_WIFI_LWIP=y
CONFIG_NVS_ALLOCATE_CACHE_IN_SPIRAM=y
CONFIG_MBEDTLS_EXTERNAL_MEM_ALLOC=y
CONFIG_SPIRAM_MALLOC_RESERVE_INTERNAL=65536
CONFIG_ESP_WIFI_STATIC_TX_BUFFER_NUM=8
CONFIG_ESP_WIFI_STATIC_RX_BUFFER_NUM=8
CONFIG_ESP_WIFI_DYNAMIC_RX_BUFFER_NUM=16
CONFIG_ESP_WIFI_MGMT_SBUF_NUM=16
CONFIG_ESP_WIFI_CACHE_TX_BUFFER_NUM=16
CONFIG_GW_ZIGBEE_UART_BAUD=230400
CONFIG_GW_ZIGBEE_UART_TX_PIN=39
CONFIG_GW_ZIGBEE_UART_RX_PIN=40
CONFIG_GW_ZIGBEE_UART_TRACE=y
CONFIG_ESP_SYSTEM_EVENT_TASK_STACK_SIZE=4096
